# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# utf-8 _without BOM_ requiured by ruby/jekyll
# eof unix type because most of website run under a unix server 
[*.html,*.css,*.js,*.htm,*.md,*.markdown]
end_of_line = crlf
insert_final_newline = true
charset = utf-8
indent_style = space
